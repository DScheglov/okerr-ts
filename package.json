{
  "name": "okerr-ts",
  "version": "0.5.2",
  "description": "Useful type to model success and failure",
  "scripts": {
    "test": "jest",
    "clear": "rm -rf ./lib; rm -rf ./dist; rm -rf ./esm",
    "compile:cjs": "tsc --project tsconfig.cjs.json",
    "compile:esm": "tsc --project tsconfig.esm.json && echo '{ \"type\": \"module\" }' > esm/package.json",
    "compile": "npm run clear; npm run compile:cjs && npm run compile:esm && tsup",
    "lint": "eslint ."
  },
  "keywords": [
    "result",
    "either",
    "monad",
    "functor",
    "async",
    "io",
    "task",
    "rust"
  ],
  "author": "Dmytro Shchehlov <dmitry.scheglov@gmail.com>",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/DScheglov/okerr-ts"
  },
  "bugs": {
    "url": "https://github.com/DScheglov/okerr-ts/issues"
  },
  "license": "MIT",
  "devDependencies": {
    "@type-challenges/utils": "^0.1.1",
    "@types/node": "^20.10.5",
    "@eslint/js": "^9.7.0",
    "@types/eslint": "^9.6.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.2.1",
    "typescript-eslint": "^7.17.0",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.2",
    "tsup": "^8.3.0",
    "typescript": "^5.6.2"
  },
  "exports": {
    ".": {
      "types": "./lib/index.d.ts",
      "import": "./esm/index.js",
      "require": "./lib/index.js"
    },
    "./base": {
      "types": "./lib/base.d.ts",
      "import": "./esm/base.js",
      "require": "./lib/base.js"
    },
    "./fn": {
      "types": "./lib/fn/index.d.ts",
      "import": "./esm/fn/index.js",
      "require": "./lib/fn/index.js"
    },
    "./do": {
      "types": "./lib/do.d.ts",
      "import": "./esm/do.js",
      "require": "./lib/do.js"
    },
    "./methods": {
      "types": "./lib/methods.d.ts",
      "import": "./esm/methods.js",
      "require": "./lib/methods.js"
    },
    "./lists": {
      "types": "./lib/lists.d.ts",
      "import": "./esm/lists.js",
      "require": "./lib/lists.js"
    },
    "./sync": {
      "types": "./lib/sync.d.ts",
      "import": "./esm/sync.js",
      "require": "./lib/sync.js"
    },
    "./sync/methods": {
      "types": "./lib/sync-methods.d.ts",
      "import": "./esm/sync-methods.js",
      "require": "./lib/sync-methods.js"
    },
    "./async": {
      "types": "./lib/async.d.ts",
      "import": "./esm/async.js",
      "require": "./lib/async.js"
    },
    "./async/methods": {
      "types": "./lib/async-methods.d.ts",
      "import": "./esm/async-methods.js",
      "require": "./lib/async-methods.js"
    }
  },
  "typesVersions": {
    "*": {
      "base": [
        "./lib/base.d.ts"
      ],
      "fn": [
        "./lib/fn/index.d.ts"
      ],
      "do": [
        "./lib/do.d.ts"
      ],
      "methods": [
        "./lib/methods.d.ts"
      ],
      "lists": [
        "./lib/lists.d.ts"
      ],
      "sync": [
        "./lib/sync.d.ts"
      ],
      "sync/methods": [
        "./lib/sync-methods.d.ts"
      ],
      "async": [
        "./lib/async.d.ts"
      ],
      "async/methods": [
        "./lib/async-methods.d.ts"
      ],
      "*": [
        "./lib/*"
      ]
    }
  },
  "files": [
    "lib",
    "esm",
    "dist"
  ]
}
